<p id="notice"><%= notice %></p>

<h1>Ramos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Profesor</th>
      <th>Creditos</th>
      <th>Materia</th>
      <th>Curso</th>
      <th>Seccion</th>
      <th>Plan estudios</th>
      <th>Connector liga</th>
      <th>Lista cruzada</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ramos.each do |ramo| %>
      <tr>
        <td><%= ramo.nombre %></td>
        <td><%= ramo.profesor %></td>
        <td><%= ramo.creditos %></td>
        <td><%= ramo.materia %></td>
        <td><%= ramo.curso %></td>
        <td><%= ramo.seccion %></td>
        <td><%= ramo.plan_estudios %></td>
        <td><%= ramo.connector_liga %></td>
        <td><%= ramo.lista_cruzada %></td>
        <td><%= link_to 'Show', ramo %></td>
        <td><%= link_to 'Edit', edit_ramo_path(ramo) %></td>
        <td><%= link_to 'Destroy', ramo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ramo', new_ramo_path %>
